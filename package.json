{
  "name": "h2o",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "build:debug": "cd android && gradlew bundleDebug",
    "build:release": "cd android && gradlew bundleRelease",
    "assemble:debug": "cd android && gradlew assembleDebug",
    "assemble:release": "cd android && gradlew assembleRelease",
    "compile": "tsc",
    "assemble:ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "deploy:android": "adb -s EHT0220518000687 install ./android/app/build/outputs/apk/release/app-universal-release.apk",
    "clean:android": "cd android && gradlew clean",
    "clean:assemble:deploy:android": "npm run clean:android && npm run assemble:release && npm run deploy:android"
  },
  "dependencies": {
